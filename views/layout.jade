doctype html
html
    head
        meta(charset='utf-8')    
    body
        ul
            if user
                li
                    a(href='/auth/logout') log out
            
            else
                li
                    a(href='/auth/login') login
                li
                    a(href='/auth/signup') sign up
        
        h1
            a(href='/main') nodejs practice page
        
        div
            p
                ul
                    each content in contents 
                        li
                            a(href='/main/' + content.id)= content.title

        block otherPage